    <% }%>
    <% if (file === 'addBlog' || file === 'editBlog') { %>
        <script src="https://cdn.jsdelivr.net/npm/marked/marked.min.js"></script>
        <title>Admin | codingtodo.com</title>
        <script src="/static/js/addBlog.js" defer></script>
    <% }%>
